{
  "name": "agentmaps",
  "version": "1.0.1",
  "description": "Geospatial agent-based modeling extension for Leaflet.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundle": "npx webpack",
    "prepublish": "npm run bundle",
    "make-docs": "cp site\\docs\\DOCS.md site\\devdocs\\DEVDOCS.md & npm run make-usr-docs & npm run make-dev-docs",
    "make-usr-docs": "jsdoc src site\\docs\\DOCS.md -d site\\docs",
    "make-dev-docs": "jsdoc src site\\devdocs\\DEVDOCS.md -d site\\devdocs -p",
    "publish-pages": "cp README.md site\\index.md & gh-pages -d site"
  },
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/noncomputable/AgentMaps.git"
  },
  "keywords": [
    "agent-based",
    "modeling",
    "simulation",
    "GIS",
    "geographic",
    "spatial"
  ],
  "author": "Andrew Tsesis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/noncomputable/AgentMaps/issues"
  },
  "homepage": "https://github.com/noncomputable/AgentMaps#readme",
  "dependencies": {
    "@turf/along": "^6.0.1",
    "@turf/bearing": "^6.0.1",
    "@turf/boolean-point-in-polygon": "^6.0.1",
    "@turf/buffer": "^5.1.5",
    "@turf/centroid": "^6.0.2",
    "@turf/destination": "^6.0.1",
    "@turf/intersect": "^6.1.3",
    "@turf/line-distance": "^4.7.3",
    "@turf/line-intersect": "^6.0.2",
    "@turf/line-slice": "^5.1.5",
    "@turf/nearest-point-on-line": "^6.0.2",
    "ngraph.graph": "0.0.14",
    "ngraph.path": "^1.0.2"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "gh-pages": "^1.2.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  }
}
